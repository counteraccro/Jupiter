<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        <link rel="stylesheet" href="{{ asset('/bundles/app/css/materialize.min.css') }}" type="text/css" />
        <script type="text/javascript" src="{{ asset('/bundles/app/js/jquery/jquery-3.2.1.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/bundles/app/js/materialize.min.js') }}"></script>
         
        {% block stylesheets %}{% endblock %}
